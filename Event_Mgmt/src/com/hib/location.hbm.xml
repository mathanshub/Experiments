<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.hib">


	<class name="Location" table="location" catalog="eventmgmt">

		<id name="loc_id">
			<column name="loc_id" />
			<generator class="assigned" />
		</id>


		<property name="loc_name" column="loc_name" />
		<property name="loc_addrs" column="loc_addrs" />


	</class>



</hibernate-mapping>
